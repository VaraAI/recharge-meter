{
    "name": "recharge-meter/recharge-meter-service",
    "description": "Laravel package for managing electrical meter recharges",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "keywords": ["laravel", "meter", "recharge", "electrical", "billing"],
    "homepage": "https://github.com/recharge-meter/recharge-meter-service",
    "support": {
        "issues": "https://github.com/recharge-meter/recharge-meter-service/issues",
        "source": "https://github.com/recharge-meter/recharge-meter-service"
    },
    "autoload": {
        "psr-4": {
            "RechargeMeter\\RechargeMeterService\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dr Constantine msigwa",
            "email": "varaai@info.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RechargeMeter\\RechargeMeterService\\Providers\\RechargeMeterServiceProvider"
            ],
            "aliases": {
                "Recharge": "RechargeMeter\\RechargeMeterService\\Facades\\Recharge"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 